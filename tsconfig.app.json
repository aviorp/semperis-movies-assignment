{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "exclude": [
    "src/**/__tests__/*"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "auto-imports.d.ts",
    "components.d.ts",
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "#build/ui/*": [
        "./node_modules/.nuxt-ui/ui/*"
      ]
    }
  },
}